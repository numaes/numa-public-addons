<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

		<!-- Scheduler for FSM Timers-->
		<record forcecreate="True" id="ir_cron_fsm_timers"
			model="ir.cron">
            <field name='name'>FSM: Main timer survillance</field>
            <field name='interval_number'>1</field>
            <field name='interval_type'>minutes</field>
            <field name="numbercall">-1</field>
            <field name="active">True</field>
            <field name="doall" eval="False" />
            <field name="model_id" ref="model_fsm_timer"/>
            <field name="state">code</field>
            <field name="code">model.schedule_timers()</field>
		</record>

    </data>
</openerp>
