<odoo>
    <data noupdate="0">
        <record id="bna_synch_scheduler" model="ir.cron">
            <field name="name">BNA Synch: get BNA currency rates</field>
            <field name="model">account.currency.bna.updater</field>
            <field name="state">code</field>
            <field name="function">synch</field>
            <field name="args"/>
            <field name='interval_number'>1</field>
            <field name='interval_type'>hours</field>
            <field name="numbercall">-1</field>
            <field name="doall">False</field>
        </record>

        <!-- BNA Currencies -->
        <record id="bna_USD" model="account.currency.bna.currency">
            <field name="name">Dolar U.S.A</field>
            <field name="currency_id" ref="base.USD"/>
            <field name="bna_units">1</field>
        </record>

        <record id="bna_GBP" model="account.currency.bna.currency">
            <field name="name">Libra Esterlina</field>
            <field name="currency_id" ref="base.GBP"/>
            <field name="bna_units">1</field>
        </record>

        <record id="bna_BRL" model="account.currency.bna.currency">
            <field name="name">Real</field>
            <field name="currency_id" ref="base.BRL"/>
            <field name="bna_units">100</field>
        </record>

        <record id="bna_EUR" model="account.currency.bna.currency">
            <field name="name">Euro</field>
            <field name="currency_id" ref="base.EUR"/>
            <field name="bna_unit">1</field>
        </record>

        <record id="bna_CHF" model="account.currency.bna.currency">
            <field name="name">Franco Suizos</field>
            <field name="currency_id" ref="base.CHF"/>
            <field name="bna_units">100</field>
        </record>

        <record id="bna_JPY" model="account.currency.bna.currency">
            <field name="name">YENES</field>
            <field name="currency_id" ref="base.JPY"/>
            <field name="bna_unit">100</field>
        </record>

        <record id="bna_CAD" model="account.currency.bna.currency">
            <field name="name">Dolares Canadienses</field>
            <field name="currency_id" ref="base.CAD"/>
            <field name="bna_units">100</field>
        </record>

        <record id="bna_DKK" model="account.currency.bna.currency">
            <field name="name">Coronas Danesas</field>
            <field name="currency_id" ref="base.DKK"/>
            <field name="bna_unit">100</field>
        </record>

        <record id="bna_NOK" model="account.currency.bna.currency">
            <field name="name">Coronas Noruegas</field>
            <field name="currency_id" ref="base.NOK"/>
            <field name="bna_units">100</field>
        </record>

        <record id="bna_SEK" model="account.currency.bna.currency">
            <field name="name">Coronas Suecas</field>
            <field name="currency_id" ref="base.SEK"/>
            <field name="bna_units">100</field>
        </record>

    </data>
</odoo>